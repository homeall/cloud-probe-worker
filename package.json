{
    "type": "module",
    "dependencies": {
        "@opentelemetry/api": "^1.7.0",
        "@opentelemetry/sdk-trace-base": "^1.16.0",
        "@opentelemetry/sdk-trace-web": "^1.16.0",
        "hono": "^4.6.5",
        "itty-router": "^3.0.11"
    },
    "description": "Global, serverless network probe endpoints with OpenTelemetry support",
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20230518.0",
        "@types/jest": "^29.5.0",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "wrangler": "^4.20.3"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "volta": {
        "node": "22.0.0"
    },
    "jest": {
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "lcov"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "**/test/**/*.test.js"
        ]
    },
    "scripts": {
        "build": "wrangler build --env production --var VERSION=${VERSION:-v1.0.0} --var GIT_COMMIT=${GIT_COMMIT:-abcdef0} --var BUILD_TIME=${BUILD_TIME:-$(date -u +'%Y-%m-%d')}"
    },
    "main": "src/index.js",
    "name": "cloud-probe-worker",
    "prettier": {
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "es5"
    },
    "scripts": {
        "deploy": "wrangler deploy",
        "dev": "wrangler dev",
        "format": "prettier --write '**/*.{js,json}'",
        "lint": "eslint . --ext .js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "version": "1.0.0"
}
